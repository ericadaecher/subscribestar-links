{
  "name": "Subscribestar Links",
  "description": "Fix the Subscribestar external links",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "https://www.subscribestar.com/*",
    "https://subscribestar.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "/icon.png",
    "32": "/icon.png",
    "48": "/icon.png",
    "128": "/icon.png"
  }
}